{
  "name": "tdp_core",
  "description": "Target discovery platform for exploring rankings of genes, disease models, and other entities.",
  "version": "13.0.1-SNAPSHOT",
  "author": {
    "name": "datavisyn GmbH",
    "email": "contact@datavisyn.io",
    "url": "https://www.datavisyn.io"
  },
  "license": "BSD-3-Clause",
  "homepage": "https://www.datavisyn.io",
  "bugs": {
    "url": "https://github.com/datavisyn/tdp_core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:datavisyn/tdp_core.git"
  },
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "phovea_registry.js",
    "dist",
    "tdp_core",
    "__init__.py",
    "__main__.py",
    "requirements.txt",
    "requirements_dev.txt",
    "docker_packages.txt",
    "redhat_packages.txt"
  ],
  "engines": {
    "npm": ">= 6.12",
    "node": ">= 12.13"
  },
  "scripts": {
    "clean": "rimraf build dist lib",
    "delete-dependencies": "rimraf node_modules",
    "check": "flake8 --exclude=.git,venv,deploy,docs,__pycache__,node_modules",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "postcompile": "npm run copy",
    "copy": "npm run copy-assets && npm run copy-styles",
    "copy-assets": "if [ -d src/assets ]; then cp -rv src/assets/. dist/assets/; fi && if [ -d src/template ]; then shx --verbose cp -R src/template/. dist/template/; fi",
    "copy-styles": "if [ -d src/scss ]; then shx --verbose cp -R src/scss/. dist/scss/; fi",
    "pretest:python": "npm run check",
    "posttest:web": "npm run lint",
    "pretest": "echo hybrid",
    "test:web": "test ! $(find tests -name \"*.ts\") || jest -w=1",
    "test:python": "test ! $(find tests -name \"*.py\") || python setup.py test",
    "test": "npm run test:web && npm run test:python",
    "lint": "eslint --cache src/**/*.ts{,x} tests/**/*.ts{,x}",
    "lint:fix": "eslint --fix --cache src/**/*.ts{,x} tests/**/*.ts{,x}",
    "docs:web": "typedoc --options typedoc.json src/**.ts",
    "docs:python": "sphinx-apidoc -o docs -f ./tdp_core && sphinx-build ./docs build/docs",
    "docs": "npm run docs:web && npm run docs:python",
    "prebuild": "echo hybrid",
    "posttest": "echo hybrid",
    "test:watch": "jest --watch",
    "build:web": "npm run clean && npm run compile",
    "build:python": "rm -rf build/source && find . -name '*.pyc' -delete && node buildPython.js && cp -r ./tdp_core build/source/",
    "build": "npm run build:web && npm run build:python",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "predist:web": "npm run build:web && npm run docs:web",
    "predist": "echo hybrid",
    "dist:python": "python setup.py sdist bdist_wheel",
    "dist": "npm run dist:web && npm run dist:python",
    "prebuild:python": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test:python",
    "prebuild:web": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test:web",
    "predist:python": "npm run build:python && npm run docs:python",
    "publish:python": "twine upload --repository-url https://upload.pypi.org/legacy/ dist/*",
    "dist:web": "mkdir lib && cd dist && tar cvzf ../lib/tdp_core.tar.gz *",
    "predocker": "npm run build",
    "docker": "docker build -t tdp_core -f deploy/Dockerfile .",
    "db-migration:base": "docker-compose run api python phovea_server/__main__.py --env dev db-migration",
    "db-migration": "npm run db-migration:base exec --",
    "db-migration:list": "npm run db-migration:base list --"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@popperjs/core": "^2.9.3",
    "bootstrap": "~5.1.0",
    "crypto-js": "^4.0.0",
    "d3": "~3.5.17",
    "i18next": "^19.8.4",
    "jquery": "~3.5.1",
    "lineupjs": "next",
    "lodash": "~4.17.20",
    "marked": "~3.0.2",
    "md5": "^2.3.0",
    "papaparse": "~5.3.0",
    "plotly.js-dist-min": "^2.8.3",
    "popper.js": "^1.15.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-highlight-words": "^0.17.0",
    "react-plotly.js": "^2.5.1",
    "react-resize-detector": "^6.7.6",
    "react-select": "^4.3.1",
    "react-sortable-hoc": "^2.0.0",
    "react-split": "^2.0.9",
    "react-table": "7.0.0-rc.16",
    "select2": "~4.0.13",
    "select2-bootstrap-theme": "0.1.0-beta.9",
    "use-deep-compare-effect": "^1.8.0"
  },
  "devDependencies": {
    "@types/d3": "~3.5.36",
    "@types/jquery": "~3.5.5",
    "@types/select2": "4.0.44",
    "@types/jest": "~26.0.20",
    "@types/bootstrap": "~5.0.15",
    "@types/react-plotly.js": "^2.5.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/papaparse": "~5.2.4",
    "@types/plotly.js-dist-min": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~27.5.1",
    "jest-raw-loader": "~1.0.1",
    "prettier": "^2.5.1",
    "rimraf": "~3.0.2",
    "shx": "~0.3.3",
    "ts-jest": "~27.1.3",
    "tslib": "~2.3.1",
    "typedoc": "~0.22.13",
    "typescript": "~4.6.2"
  }
}
