{
  "name": "tdp_core",
  "description": "Target discovery platform for exploring rankings of genes, disease models, and other entities.",
  "version": "15.1.1-SNAPSHOT",
  "author": {
    "name": "datavisyn GmbH",
    "email": "contact@datavisyn.io",
    "url": "https://www.datavisyn.io"
  },
  "license": "BSD-3-Clause",
  "homepage": "https://www.datavisyn.io",
  "bugs": {
    "url": "https://github.com/datavisyn/tdp_core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:datavisyn/tdp_core.git"
  },
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "tdp_core",
    "requirements.txt",
    "requirements_dev.txt"
  ],
  "engines": {
    "npm": ">=8",
    "node": ">=16"
  },
  "scripts": {
    "all": "npm run lint:fix && npm run test && npm run build && npm run dist",
    "build": "npm run clean && npm run compile && npm run copy",
    "ci": "npm run lint && npm run test && npm run build && npm run dist",
    "clean": "rimraf build dist lib",
    "compile:watch": "visyn_scripts compile --watch",
    "compile": "visyn_scripts compile",
    "copy": "visyn_scripts copy",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "delete-dependencies": "rimraf node_modules",
    "dist": "mkdir lib && cd dist && tar cvzf ../lib/tdp_core.tar.gz *",
    "docs": "visyn_scripts docs",
    "lint:fix": "visyn_scripts lint --fix",
    "lint": "visyn_scripts lint",
    "predist": "npm run lint && npm run test && npm run build",
    "prepare": "npm run build",
    "start": "visyn_scripts start --env workspace_mode=single",
    "storybook:build": "NODE_OPTIONS=--max_old_space_size=4096 build-storybook",
    "storybook": "NODE_OPTIONS=--max_old_space_size=4096 start-storybook -p 6006",
    "test": "visyn_scripts test",
    "webpack:dev": "visyn_scripts build --mode development --env workspace_mode=single",
    "webpack:prod": "visyn_scripts build --mode production --env workspace_mode=single"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@popperjs/core": "^2.9.3",
    "@types/bootstrap": "~5.0.15",
    "@types/d3": "~3.5.36",
    "@types/jquery": "~3.5.5",
    "@types/papaparse": "~5.2.4",
    "@types/plotly.js-dist-min": "^2.3.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-plotly.js": "^2.5.0",
    "@types/select2": "4.0.44",
    "bootstrap": "~5.1.0",
    "crypto-js": "^4.0.0",
    "d3": "~3.5.17",
    "i18next": "^19.8.4",
    "jquery": "~3.5.1",
    "lineupjs": "4.6.2",
    "lodash": "~4.17.20",
    "marked": "~3.0.2",
    "md5": "^2.3.0",
    "papaparse": "~5.3.0",
    "plotly.js-dist-min": "^2.8.3",
    "popper.js": "^1.15.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-highlight-words": "^0.17.0",
    "react-plotly.js": "^2.5.1",
    "react-resize-detector": "^6.7.6",
    "react-select": "^4.3.1",
    "react-sortable-hoc": "^2.0.0",
    "react-split": "^2.0.9",
    "react-table": "^7.0.4",
    "select2": "~4.0.13",
    "select2-bootstrap-theme": "0.1.0-beta.9",
    "use-deep-compare-effect": "^1.8.0",
    "visyn_scripts": "git+ssh://git@github.com:datavisyn/visyn_scripts.git#develop"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/plugin-transform-typescript": "7.17.12",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "0.0.9",
    "cypress": "^9.5.2",
    "local-cypress": "^1.2.5"
  },
  "overrides": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "resolutions": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "visyn": {
    "entries": {
      "app": {
        "js": "src/demo/index.initialize.tsx",
        "html": "index.html",
        "template": "src/demo/index.template.ejs",
        "chunkName": "app",
        "excludeChunks": []
      }
    }
  }
}
