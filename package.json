{
  "name": "tdp_core",
  "description": "Target discovery platform for exploring rankings of genes, disease models, and other entities.",
  "version": "15.1.1-SNAPSHOT",
  "author": {
    "name": "datavisyn GmbH",
    "email": "contact@datavisyn.io",
    "url": "https://www.datavisyn.io"
  },
  "license": "BSD-3-Clause",
  "homepage": "https://www.datavisyn.io",
  "bugs": {
    "url": "https://github.com/datavisyn/tdp_core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:datavisyn/tdp_core.git"
  },
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "phovea_registry.js",
    "dist",
    "tdp_core",
    "requirements.txt",
    "requirements_dev.txt"
  ],
  "engines": {
    "npm": ">= 6.12",
    "node": ">= 12.13"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf build dist lib",
    "compile:watch": "tsc -w",
    "compile": "tsc",
    "copy-assets": "if [ -d src/assets ]; then cp -rv src/assets/. dist/assets/; fi && if [ -d src/template ]; then shx --verbose cp -R src/template/. dist/template/; fi",
    "copy-styles": "if [ -d src/scss ]; then shx --verbose cp -R src/scss/. dist/scss/; fi",
    "copy": "npm run copy-assets && npm run copy-styles",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "delete-dependencies": "rimraf node_modules",
    "dist": "mkdir lib && cd dist && tar cvzf ../lib/tdp_core.tar.gz *",
    "docs": "typedoc --options typedoc.json src/**.ts",
    "lint:fix": "eslint --fix --cache src/**/*.ts{,x} tests/**/*.ts{,x}",
    "lint": "eslint --cache src/**/*.ts{,x} tests/**/*.ts{,x}",
    "postcompile": "npm run copy",
    "posttest": "npm run lint",
    "prebuild": "npm run test",
    "predist": "npm run build && npm run docs",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "test:watch": "jest --watch",
    "test": "test ! $(find tests -name \"*.ts\") || jest -w=1",
    "make": "make --",
    "storybook": "NODE_OPTIONS=--max_old_space_size=4096 start-storybook -p 6006",
    "storybook:build": "NODE_OPTIONS=--max_old_space_size=4096 build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@popperjs/core": "^2.9.3",
    "bootstrap": "~5.1.0",
    "crypto-js": "^4.0.0",
    "d3": "~3.5.17",
    "d3-array": "^2.7.1",
    "d3-axis": "^1.0.8",
    "d3-brush": "^1.0.4",
    "d3-drag": "^1.2.1",
    "d3-format": "^1.3.0",
    "d3-polygon": "^1.0.3",
    "d3-quadtree": "^1.0.3",
    "d3-scale": "^2.1.0",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "d3-zoom": "^1.7.1",
    "eventemitter3": "^4.0.7",
    "i18next": "^19.8.4",
    "jquery": "~3.5.1",
    "lineupjs": "4.6.2",
    "lodash": "~4.17.20",
    "marked": "~3.0.2",
    "md5": "^2.3.0",
    "papaparse": "~5.3.0",
    "plotly.js-dist-min": "^2.8.3",
    "popper.js": "^1.15.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-highlight-words": "^0.17.0",
    "react-plotly.js": "^2.5.1",
    "react-resize-detector": "^6.7.6",
    "react-select": "^4.3.1",
    "react-sortable-hoc": "^2.0.0",
    "react-split": "^2.0.9",
    "react-table": "^7.0.4",
    "select2": "~4.0.13",
    "select2-bootstrap-theme": "0.1.0-beta.9",
    "use-deep-compare-effect": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "0.0.9",
    "@types/bootstrap": "~5.0.15",
    "@types/d3": "~3.5.36",
    "@types/jest": "~27.4.1",
    "@types/jquery": "~3.5.5",
    "@types/papaparse": "~5.2.4",
    "@types/plotly.js-dist-min": "^2.3.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-plotly.js": "^2.5.0",
    "@types/select2": "4.0.44",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-loader": "^8.2.3",
    "css-loader": "~4.3.0",
    "cypress": "^9.5.2",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-raw-loader": "~1.0.1",
    "local-cypress": "^1.2.5",
    "prettier": "^2.5.1",
    "rimraf": "~3.0.2",
    "sass": "1.52.1",
    "sass-loader": "~9.0.2",
    "shx": "~0.3.3",
    "style-loader": "~2.0.0",
    "ts-jest": "~27.1.3",
    "tslib": "~2.3.1",
    "typedoc": "~0.22.13",
    "typescript": "~4.6.2"
  }
}
